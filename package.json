    "db:seed": "cross-env SEED=true NODE_ENV=development ts-node src/database/build.ts",
    "db:seed:production": "cross-env SEED=true NODE_ENV=production ts-node src/db/build.ts",
    "test": "cross-env NODE_ENV=test jest -i"
